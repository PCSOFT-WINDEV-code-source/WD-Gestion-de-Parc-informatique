  procedures :
   -
     name : CréationLieu
     procedure_id : 1744741193111057519
     type_code : 14
     code : |1-
      // Résumé : Création d'un nouveau lieu
      // Syntaxe :
      //CréationLieu ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure CréationLieu()
      
      SI gnx1 = -1 ALORS
      	
      	//On met à jour le coin haut à gauche du lieu concerné
      	gnx1 = SourisPosX()
      	gny1 = SourisPosY()
      	ToastAffiche(<§$0001§>)
      	
      SINON
      	
      	si gnX2 = -1 alors
      		
      		//On met à jour le coin bas à droite du lieu concerné
      		gnx2 = SourisPosX()
      		gny2 = SourisPosY()	
      		
      		SI bSuperposéAvecAutreLieu() ALORS
      			
      			//Deux lieux se superposent
      			Info(<§$0002§>)
      			ExécuteTraitement(BTN_Nouveau, trtclic)
      			
      		sinon
      			
      			//On dessine le bureau
      			dRectangle(gnX1,gnY1,gnX2,gnY2,Transparent,RougeFoncé)
      			
      			//1 : Il convient
      			//2 : Il ne convient pas
      			SELON Dialogue(<§?183694D20182A3D4§>)
      			
      				// Il est correct.
      				CAS 1 :
      					
      					//On met à jour la valeur de la rubrique contenant les coordonnées
      					LIEU.Rectangle = NumériqueVersChaîne(gnX1)+","+NumériqueVersChaîne(gnY1)+","+NumériqueVersChaîne(gnX2)+","+NumériqueVersChaîne(gnY2)
      					toastaffiche(<§$000d§>, toastCourt, cvMilieu,chCentre)
      							
      					
      				// Il est incorrect.
      				CAS 2 :
      					AffichePlan(IMG_PlanLocaux)
      					ExécuteTraitement(BTN_Nouveau, trtClic)
      					
      			FIN	
      			
      		FIN	
      		
      	fin
      FIN
     type : 458752
   -
     name : bSuperposéAvecAutreLieu
     procedure_id : 1744742176659321741
     type_code : 14
     code : |1-
      // Résumé : Vérifie si le lieu en cours de création se superpose avec un autre existant
      // Syntaxe :
      //[ <Résultat> = ] bSuperposéAvecAutreLieu ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	booléen : Vrai si le lieu se superpose avec un autre
      //
      
      Procédure bSuperposéAvecAutreLieu()
      
      nX1, nY1, nX2, nY2 sont des entiers
      
      //On parcourt chaque lieu pour vérifier que le lieu en cours de création ne se superpose pas avec un existant
      POUR TOUT LIEU 
      	
      	nX1 = ExtraitChaîne(LIEU.Rectangle,1,",")
      	nY1 = ExtraitChaîne(LIEU.Rectangle,2,",")
      	nX2 = ExtraitChaîne(LIEU.Rectangle,3,",")
      	nY2 = ExtraitChaîne(LIEU.Rectangle,4,",")
      	
      	SI pas ((gnx1 < nX1 ET gnx2 < nX1) OU (gnx1 > nX2 ET gnx2 > nX2) OU (gny1 < nY1 ET gny2 < nY1) OU (gny1 > nY2 ET gny2 > nY2)) ALORS
      		
      		RENVOYER vrai	
      		
      	FIN	
      	
      FIN
      
      RENVOYER faux
     type : 458752
   -
     name : SuppressionLieu
     procedure_id : 1744749972043621321
     type_code : 14
     code : |1+
      // Résumé : Supprime le lieu en cours, celui sur lequel on a cliqué dans le plan
      // Syntaxe :
      //SuppressionLieu ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure SuppressionLieu()
      
      //1 : &Supprimer
      //2 : &Ne pas supprimer
      SELON Dialogue(<§?1836921B00B206DA§>, SAI_Emplacement)
      
      	// &Supprimer
      	CAS 1 :
      		
      		//On vérifie si le lieu ne correspond à aucune machine
      		HLitRecherchepremier(MACHINE, IDLieu, lieu.IDLieu)
      		SI PAS HTrouve(MACHINE) ALORS
      			HSupprime(LIEU)
      		SINON
      			Info("Des machines sont associées à ce lieu. Vous ne pouvez donc pas le supprimer.")
      		FIN
      		
      		//On réinitialise l'affichage des informations du lieu
      		raz()
      		IMG_Picto = ""
      		
      		// Rafraîchissement du plan des locaux
      		AffichePlan(IMG_PlanLocaux)
      		
      	// &Ne pas supprimer : on ne fait rien
      	CAS 2 :
      		
      FIN
     type : 458752
