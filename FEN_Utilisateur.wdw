  -
    name : BTN_OK
    identifier : 0x283a808ddc6ab15c
    internal_properties : 
    properties :
     x : 490
     y : 412
     width : 38
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         //On vérifie qu'au moins le nom de l'utilisateur a été saisi
         SI SAI_Utilisateur = "" ALORS
         	Info("Veuillez saisir au moins le nom de l'utilisateur.")
         	RepriseSaisie(SAI_Utilisateur)
         FIN
         
         //ainsi que toutes les informations saisies
         EcranVersFichier(MaFenêtre, UTILISATEUR)
         
         sCheminPhoto est une chaine
         //On remplit la rubrique photo avec celle spécifiée par l'utilisateur
         SI SAI_Photo <> "" ALORS
         	sCheminPhoto = SAI_Photo
         SINON	
         	SELON COMBO_Civilité
         		CAS 1, 2 :
         			sCheminPhoto = ComplèteRep(fRepexe()) + "Images\visage_femme.gif"  			
         		AUTRE CAS :
         			sCheminPhoto = ComplèteRep(fRepExe()) + "Images\visage_homme.gif"  
         	FIN
         FIN
         
         //On met la photo sélectionnée dans le fichier
         HAttacheMémo(UTILISATEUR,photo,sCheminPhoto)
         
         
         //On ajoute ou modifie l'utilisateur
         si UTILISATEUR..NouvelEnregistrement = vrai alors
         	
         	hajoute(UTILISATEUR)
         	//A la fermeture de la fenêtre, on renvoie l'utilisateur créé
         	mafenêtre..ValeurRenvoyée = UTILISATEUR.Initiales
         	
         sinon
         	
         	HModifie(UTILISATEUR)		
         	
         FIN
         
         //On re-grise les champs de saisie
         GR_AjoutModif..Etat = Grisé
         
         //on met à jour la table
         TableAffiche(TABLE_Utilisateurs, taCourantEnreg)
         
        type : 18
      -
        type : 161
        enabled : false
