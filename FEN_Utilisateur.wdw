  -
    name : BTN_Supprimer
    identifier : 0x283a808ddc69b15c
    internal_properties : 
    properties :
     x : 462
     y : 12
     width : 96
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         //1 : &Supprimer
         //2 : &Ne pas supprimer
         SELON Dialogue(<§?183A7DE101613D07§>, TABLE_Utilisateurs.COL_Utilisateur)
         	
         	// &Supprimer
         	CAS 1 :	
         		
         		//On vérifie si l'utilisateur possède des machines
         		HLitRecherchePremier(MACHINE, InitialesUtilisateur, TABLE_Utilisateurs.COL_Utilisateur)
         		SI PAS HTrouve(MACHINE) ALORS
         			
         			//Suppression de l'utilisateur dans la table => aussi dans le fichier	
         			TableSupprime(TABLE_Utilisateurs)
         			FichierVersEcran(MaFenêtre, UTILISATEUR)
         			
         		SINON
         			Info("Des machines sont associées à cet utilisateur. Vous ne pouvez donc pas le supprimer.")
         		FIN           
         		
         	// &Ne pas supprimer : on ne fait rien
         	CAS 2 :
         		
         FIN
        type : 18
      -
        type : 161
        enabled : false
