    identifier : 0x283a808ddc6bb15c
    internal_properties : 
    properties :
     x : 534
     y : 412
     width : 38
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         //On remet à jour les champs de saisie avec les valeurs présentes dans le fichier
         fichierversecran(mafenêtre, UTILISATEUR) 
         
         //On re-grise les champs de saisie
         GR_AjoutModif..etat = grise
        type : 18
